@charset "utf-8";

@import "base";

.clear{clear:both;display: block}
.content{
  margin-top: 20px;
}
.content-search{
    width:737px;
    margin:20px auto;
    border:1px solid #ccc;
    position:relative;
    #warnning{
              display: none;
              position: absolute;
              width:250px;
              height: 74px;
              background-color:#fffde1;
              left:243px;
              top:230px;
              line-height: 74px;
              text-align: center;
              box-shadow: 0 0 10px rgba(0,0,0,.4);
              z-index: 998;
    }
}
.loading{
    text-align: center;
}
.tip{
    color: #ff7900;
    .ft12_w();
    height: 30px;
    line-height: 30px;
    text-align: center;
}
.tab_tit{
    height: 40px;
    line-height: 40px;
    background-color:#f3f3f3;
    text-align: center;
    border-bottom: 1px solid #ccc;
    span{
        padding: 0 30px;
        cursor: pointer;
        height: 40px;
        line-height: 40px;
        display: block;
        float: left;
        .ft12_w();
    }
    .excel{width:100px;text-align: center;}
    label{color:#c00;font-size:12px}
    span.active{
        color: #c00;
        background: #fff;
        border-bottom:1px solid #fff;
        border-left:1px solid #ccc;
        border-right:1px solid #ccc;
    }
}
.tab_con{
    padding:10px 20px;
    background: #fff;
    width: 695px;
    div{
        display: none;
    }
    div.active, div.active div{
        display: block;
    }
    .area_left{
        width: 448px;/**/
        float: left;
    }
    .area_right{
        float: left;
        padding: 70px 0;
    }
    table{margin:auto}
    table .space{padding-left:15px}
    table tr td{
        padding: 5px;
        font-size:13px;
        font-family: Microsoft YaHei;
        font-weight: normal;
        color: #333;
        p{
            height: 25px;
            line-height: 25px;
        }
    }
    .form_bottom{position: relative;display: block;margin-left:52px;line-height: 29px;padding:10px 0;width:643px}
    /*.tab2 .form_bottom,.supplier{margin-left:0}*/
    .bottom_left{
      font-size:13px;
      font-family: Microsoft YaHei;
      color:#333;
      float:left;
      width:286px;
      .place{
        display: block;
        margin-bottom:10px;
      }

    }
    .supplier{
      width: 295px;
      margin-left:20px;
      display: block;
      float:left;
      font-size:13px;
      .select_supplier{
        width: 14px;
        height:10px;
        background:url(../img/supplier_down.png) no-repeat center;
        border:none;
        cursor: pointer;
        *vertical-align: middle;
      }
      .select_border{
        display: none;
        font-size:12px;
        border:1px solid #999;
        padding:10px;
        color:#666;
        position:absolute;
        left:0;
        background-color:#fff;
        width:600px;
        z-index: 998;
        ul li{
          list-style: none;
          float:left;
          width: 120px;
          height:25px;
          color:#333;
          text-align: left;
          input{
            vertical-align: middle;
          }
          label{
            cursor:pointer
          }
          label:first-child input{
            vertical-align: text-bottom;
          }
        }
        .close{
          color:#999;
          position:absolute;
          right:2px;
          top:5px;
          width: 15px;
          height:15px;
          display: inline-block;
          text-align: center;
          line-height: 11px;
          cursor:pointer;
          font-size:20px;
          font-family:arial;
        }
      }
      .promt{display: block;float:left}
      .show_supplier{
          display: block;
          float:left;
          border:1px solid #ccc;
          max-width: 211px;
          span{
            width:103px;
            height:27px;
            display: inline-block;
            font-size:13px;
            padding:0;
            line-height: 27px;
            text-align: center;
          }
          span[onclick*="show_selectBorder"]{
            cursor:pointer;
          }
      }
    }
    input[type="text"]{
        width: 245px;
        height: 25px;
        line-height: 25px;
        .ft12_w();
        border: 1px solid #ccc;
    }
    input[name='mult_par']{
      text-transform:uppercase;
    }
    .prompt{
        border: 1px solid #f4c8c8;
        /*box-shadow: 0 0 2px #FF6666;*/
        background-color:#ffe9e9;
    }
    .reset_btn{
        border: none;
        width: 70px;
        height: 33px;
        line-height: 33px;
        background: #ccc;
        color: #fff;
        float:right;
        font-family: Microsoft YaHei;
        margin-bottom:8px;
        cursor:pointer;
        font-size: 14px;
    }
    .sub_btn{
        border: none;
        width: 100px;
        height:33px;
        line-height: 33px;
        background: #c00;
        color: #fff;
        font-family: Microsoft YaHei;
        float:left;
        margin-bottom:8px;
        cursor:pointer;
        font-size:14px;
    }
    .tip{
        color: #ff7900;
        .ft12_w();
        height: 30px;
        line-height: 30px;
    }
    textarea{
        border: 1px solid #ccc;
        width: 390px;
        height: 378px;/*398*/
        margin: 5px;
        margin-left:52px;
        resize:none;
    }
    .area_right{
        h3{
            .ft12_w();
            color: #999;
            height: 30px;
            line-height: 30px;
        }
    }
    .area_left{
        p{
          height:33px;
          span{
              .ft12_w();
              color:#666;
              padding: 0 3px;
              float:left;
              margin-top:8px;
          }
        }
    }
    .radio_c{
        width: 12px;
        height: 12px;
        border: none;
        vertical-align:text-top;
    }
}
.shade{position:absolute;top:0;left:0;width:100%;height:100%;display: none}

