//登录
.search_login{
    display: none;
    width:400px;
    height:275px;
    position: fixed;
    background-color:#fff;
    font-family:Microsoft YaHei;
    font-weight: normal;
    z-index: 998;
    font-weight: bold;
    box-shadow: 0 0 15px rgba(0,0,0,.8);
    .title{
      width: 100%;
      height:35px;
      background:#c00;
      color:#fff;
      font-size:14px;
      line-height: 35px;
      text-align: center;
      margin-bottom:30px;
      position: relative;
    }
    .ajax_input{
      text-align: center;
      height:50px;
      color:#333;
      font-size: 12px;
      label{
        font-family:Times New Roman;
      }
      input{
        width: 238px;
        height: 28px;
        border:1px solid #ccc;
        border-radius:5px;
      }
      .link{
        display: inline-block;
        width: 100px;
        a{text-decoration: underline;color:#666;}
      }
    }
    .last{
      height: 35px;
    }
    .ajax_login{
      display: inline-block;
      width: 102px;
      margin:auto;
      height:35px;
      line-height: 35px;
      text-align: center;
      background-color:#c00;
      color:#fff;
      cursor:pointer;
      font-size:14px;
      border-radius:5px;
    }
    #js-captcha-refresh{
      cursor:pointer;
    }
    .close{
      width:30px;
      height:30px;
      position: absolute;
      top:0;
      right: 0;
      color:#fff;
      font-size:18px;
      font-family: arial;
      cursor:pointer;
    }
    .error_info{
      width: 100%;
      text-align: center;
      color:#c00;
      font-size: 12px;
      display: none;
      height: 30px;
      line-height: 30px;
    }
  }
.shadow{
    display: none;
    position: fixed;
    width: 100%;
    height: 100%;
    top:0;
    left:0;
    background-color:rgba(0,0,0,.5);
