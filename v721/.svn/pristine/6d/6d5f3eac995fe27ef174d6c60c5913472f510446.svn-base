/* 模糊查询结果样式 */
.blur-info {
  background: #fff;
  border-top: 1px dotted #ddd;
  .blur-tit {
    overflow: hidden;
    h3 {
      color: #d50000;
      float: left;
      margin: 10px 20px;
      display: inline;
    }
    a {
      float: right;
      margin: 10px 20px;
    }
  }
  .blur-res {
    position: relative;
    overflow: hidden;
    //display: none;
    .mult_tit {
      height: auto;
      background: #fff;
    }
    .blur-box {
      width: 1080px;
      border-top: 1px solid #000;
      position: absolute;
      left: 0;
      top: 40px;
      z-index: 9999;
      display: none;
      .mult_con {
        width: 1080px !important;
      }
    }
    @media screen and (max-width:1024px) {
      .blur-box {
        width: 960px;
        .mult_con {
          width: 960px !important;
        }
      }
    }
    #blur-data li {
      width: 155px;
      float: left;
      padding: 5px 30px;
      font-weight: 500;
      a {
        position: relative;
        padding: 8px 15px;
        display: inline-block;
        span.close-blur {
          position: absolute;
          top: -5px;
          right: 0;
          color: #fff;
          font-size: 18px;
          display: none;
        }
      }
      .blur-active {
        background: #333;
        color: #fff;
      }
    }
  }
}
/*
左侧栏导航样式
*/
.left_filter {
    position: fixed;
    left: 62px;
    top: 122px;
    z-index: 999;
    li {
        width: 78px;
        height: 38px;
        position: relative;
        a {
            display: block;
            background: #f3f3f3;
            text-align: center;
            line-height: 38px;
            border: 1px solid #e3e3e3;
            border-left: none;
        }
        a.close-sup {
            position: absolute;
            top: 5px;
            right: 0;
            line-height: 0;
            font-size: 20px;
            display: none;
        }
    }
    .active a {
        color: #fff;
        background: @ahover-color;
        border: none;
    }
}
