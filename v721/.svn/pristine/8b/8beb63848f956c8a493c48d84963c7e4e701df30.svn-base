@charset "utf-8";
@import "base";

.topAdv{
  width:1160px;
  height: 30px;
  line-height: 30px;
  text-align:left;
  margin: 0 auto;
  background:#fee4a7;   //#ffe468
  position: relative;
  color: #000;
  .ft12_w();
  padding-left: 30px;
  span{
    position: absolute;
    top: -3px;
    left: -33px;
  }
  a{
    color: #b60000;
    /*background: url(../img/close.png) 28px 5px no-repeat;*/
    width: 55px;
    height: 30px;
    display: block;
    position: absolute;
    right: 0;
    top: 0;
    text-align: right;
  }
}
.left{
  float: left;
  width: 210px;
  padding: 10px 20px 0 20px;
  position: relative;
  border: 1px solid #CCC;
  overflow: hidden;
  h3{
    .ft14_w();
    color: #333;
    height: 32px;
    line-height: 32px;
    border-bottom: 1px solid #ccc;
    span{
      height: 32px;
      line-height: 32px;
      display: inline-block;
      border-bottom: 1px solid @ahover-color;
    }
  }
  ul{
    width: 100%;
    overflow: hidden;
    li{
      width: 100%;
      padding-top: 10px;
      padding-bottom: 18px;
      border-bottom: 1px dotted #ccc;
      position: relative
    }
    li.last{
      border:none;
    }
    img{
      width: 140px;
      height: 52px;
    }
    span{
      background: #ff9231;
      color: #fff;
      .ft12_w();
      padding: 1px 10px;
      position: absolute;
      top: 27px;
      right: 0px;
    }
    .fz16{
      font-size: 16px;
      padding: 0px 2px;
    }
  }
}
.right{
  width: 920px;
  float: right;
  position: relative;
  overflow: hidden;
  .search{
    width: 100%;
    height: 154px;
    overflow: hidden;
    .con{
      margin-top: 40px;
      margin-left: 88px;
      height: 42px;
      overflow: hidden;
      .srcform{
        height: 38px;
        border: 2px solid #CC0000;
        float: left;
        overflow: hidden;
      }
      input{
        background: none;
        border: none;
        float: left;
        // display: inline;
        font-size: 16px;
        font-family: "Helvetica-Neue", "Helvetica", Arial, sans-serif;
        height: 38px;
        line-height:38px;
        margin: 0px;
        padding: 0px;
        border-radius: 0;
      }
      .partno{
        width: 284px;
        // width: 49%;
        padding-left: 34px;
        border-right: 1px solid #ccc;
        text-transform: uppercase;
        background: url(../img/search_bg.jpg) left center no-repeat;
      }
      .qty{
        width: 93px;
        // width: 16%;
        padding-left: 30px;
      }
      .submit_btn{
        width: 138px;
        // width: 35%;
        height: 38px;
        cursor: pointer;
        background: #CC0000;
        color: #fff;
        -webkit-appearance: none;
      }
      .bom_btn{
        float: left;
        font-size:16px;
        background-color: #ff7700;
        color: #fff;
        height: 42px;
        margin-left: 20px;
        line-height: 42px;
        padding: 0 24px 0 26px;
        display: block;
        border-radius: 0;
        -webkit-appearance: none;
      }
      .clearfix();
    }
    p{
      margin-left: 88px;
      margin-top: 5px;
      a{
        font-family: '微软雅黑';
        font-size: 13px;
        color: #666;
        letter-spacing: 1px;
        height: 22px;
        line-height: 22px;
        padding: 0 7px;
      }
    }
  }
  .banner{
    width: 100%;
    height: 260px;
    position: relative;
    overflow: hidden;
    ul{
      float: left;
    }
    .small{
      li{
        width: 240px;
        height:130px;
        overflow: hidden;
      }
      img{
        width: 240px;
        height: 130px;
      }
    }
    .big{
      li{
        width: 680px;
        height: 260px;
        overflow: hidden;
      }
      img{
        width: 680px;
        height: 260px;
      }
    }
    .icon{
      position: absolute;
      bottom: 10px;
      left: 330px;
      a{
        width: 9px;
        height: 9px;
        display: inline;
        float: left;
        margin: 0 3px;
        overflow: hidden;
        background: url("../img/icon_1.png") no-repeat;
        background-position: top right;
      }
      a.active{
        background:  url("../img/icon_1.png") no-repeat top left;
      }
    }
  }
  .link{
    margin-top: 22px;
    vertical-align:baseline;
    padding: 0 20px;
    dl{
      width: 175px;
      float: left;
      margin-right: 60px;
      display: inline;
      dt{
        height: 30px;
        line-height: 30px;
        border-bottom: 1px solid #CCC;
      }
      span{
        height: 30px;
        line-height: 30px;
        display: inline-block;
        border-bottom: 1px solid @ahover-color;
        color: #333;
        .ft14_nw()
      }
      dd{
        margin-top: 15px;
        .ft12_w();
        color: #333;
      }
      a:hover{
        color:@ahover-color;
      }
    }
    dl.last{
      margin: 0;
    }
  }
}

// 用户中心的样式
.accounts{
  width: 100%;
  float:left;
  margin-left: 12px;
  //margin-top: 20px;
}
//登录
.login{
  width: 449px;
  margin: 0 auto;
  padding:0 30px 40px 30px;
  border: 1px solid #ccc;
  text-align: center;
  h3{
    color:@main-color;
    font-size:14px;
    font-family: simsun;
    height: 54px;
    line-height: 54px;
    border-bottom: 1px dotted #ccc;
    position: relative;
    z-index: 2;
    .sign{
      display: inline-block;
      width: 24px;
      height: 23px;
      position:relative;
      top:7px;
      margin-right:7px;
      background-image: url(../img/login_sign.png);
      *top:3px;
    }
    .sign_pwd{
      width: 18px;
      background-image: url(../img/lock.png);
    }
    p{
      display: inline-block;
      width: 120px;
      position: absolute;
      right:0;
      top:0;
      text-align: right;
      font-weight: normal;
      a{font-size: 14px;}
      span{color:#838383}
    }
    #error{
      display: inline-block;
      position: absolute;
      background: #fff1f1;
      top:11px;
      z-index: 8;
      padding:0 15px;
      height: 32px;
      border:1px solid #ee9799;
      line-height: 32px;
      color:#333;
      font-size:12px;
      span{
        display: inline-block;
        width: 18px;
        height: 18px;
        border-radius: 9px;
        color:#fff;
        text-align: center;
        line-height: 18px;
        background:@main-color;
        margin-right: 10px;
        font-family: arial;
        position: relative;
      }
      a{color:@main-color;font-size:12px}
    }
  }
  ul {
    padding: 20px;
    input{text-indent: 5px;}
  }
  .forgetpwd{
    padding:20px 5px;
    input[type='text']{font-size:12px;font-family: simsun}
    input[type='text']:focus,input[type='password']:focus{border-color:#ccc;box-shadow: none}
    .correct{display: inline-block;width: 12px;height: 12px;*position:relative;*top:-10px;background-image: url(../img/register_validate.jpg);background-position: 0px 12px;margin-left:10px;}
    .error{display: inline-block;width: 12px;height: 12px;*position:relative;*top:-10px;background-image: url(../img/register_validate.jpg);background-position: 0px 0px;margin-left:10px;}
    p{display: inline;color:#c00;margin-left:5px;position: relative;top:-2px;*top:-10px;font-size: 12px;font-family: simsun}
    #validate_border{
      width: 200px;
      height: 30px;
      border:1px solid #ccc;
      display: inline-block;
      vertical-align: middle;
      *float:left;
      input{width: 97px;height: 28px;border:none;line-height: 28px;padding:0;*float:left}
      span{display: inline-block;width: 102px;height: 30px;background:#f2f2f2;text-align: center;line-height:30px;border-left:1px solid #ccc;font-size: 12px;font-family: simsun;cursor:pointer;}
    }
    #des{width: 105px;height: 28px;display: none;background-color:#f2f2f2;line-height: 28px;text-align: center;border:1px solid #ccc;margin-left:10px;color:#838383;font-size:12px;font-family: simsun}
    #des b{font-weight:normal;}
  }
  li {
    margin: 10px 0;
    text-align: left;
    line-height: 32px;
    height: 32px;
  }
  .item{
    color: #333;
    font-size: 12px;
    font-family: simsun;
    width: 52px;
    text-align: right;
    padding-right:10px;
    display: inline-block;
    margin-left:46px;
    *vertical-align: top;
    *float:left;
    *margin-right: 8px;
  }
  #captcha_img{margin-bottom:-8px}
  input {
    height: 28px;
    line-height: 28px;
    width: 198px;
    border: 1px solid #ccc;
  }
  input[type='text']:focus,input[type='password']:focus{border:1px solid #ee9799;box-shadow: 0 0 2px #c00;-webkit-box-shadow:0 0 0px 50px white inset !important;}
  #js-captcha-refresh{
    cursor:pointer;
    position: relative;
    top:4px;
  }
  .sub_btn {
    height: 37px;
    width: 110px;
    background: @main-color;
    border: none;
    color: #fff;
    font-size: 14px;
    font-family: simsun;
    font-weight: bold;
    cursor:pointer;
  }
  a {
    font-size:12px;
    font-family: simsun;
    color: #666;
    padding: 0 5px;
  }
  .re_span {
    width: 80px;
    padding-right: 5px;
    text-align: right;
  }
  .re_input {
    width: 180px;
  }
  .re_code {
    border: none;
    background: #fee4a7;
    width: 70px;
    .ft12_w();
    color:#333;
  }
  .login_btn{margin-bottom:20px}
}
//注册
.register{
  width:790px;
  height: 710px;
  border:1px solid #ccc;
  margin:auto;
  padding:16px 30px;
  .top{
    width:100%;
    height: 23px;
    line-height: 23px;
    font-family: simsun;
    font-size:14px;
      i{
        display: inline-block;
        width: 24px;
        height: 23px;
        vertical-align: top;
        background-image:url(../img/login_sign.png);
      }
      #title{
        color:#c00;
        font-weight: bold;
        clear:both;
        margin:0 20px 0 10px;
      }
      span{
        color:#838383;
      }
      a{
        color:#333;
        margin-left:10px;
      }
      span,a{
        display: inline-block;
        height: 23px;
        line-height: 23px;
      }
  }
  .content{
     width: 100%;
     margin-top:16px;
     border-top:1px #ccc dotted;
     font-family: simsun;
     font-size:12px;
     padding-top: 35px;
     ul li{
       list-style: none;
       width: 100%;
       height: 30px;
       margin-bottom:20px;
       line-height: 30px;
       .item{
         width: 108px;
         text-align: right;
         display: inline-block;
         margin-right: 8px;
         *float:left;
         span{color:#c00;margin:0 5px 0 0;display: inline;}
       }
       a{color:#c00}
       span{color:#838383;margin-left:10px;display: none;*position:relative;top:-9px;}
       span:first-child{*position:relative;top:0px;}
       label{margin:0 60px 0 5px;}
       input[type='radio'],input[type='checkbox']{vertical-align: -3px}
       input[type='text']{font-family: simsun;font-size:12px;}
       input[type='text'],input[type='password']{width:218px;height: 28px;border:1px solid #ccc;text-indent: 4px;line-height: 28px;padding:0;}
       input[name='file']{width: 149px;border:none;height: 27px;*float:left}
       input[name='upload']{display: none}
       input[name='email']{*float:left}
       p{display: inline;color:#c00;margin-left:5px;position: relative;top:-2px;*top:-9px}
       #border{width: 218px;height:28px;border:1px solid #ccc;display: inline-block;*float:left}
       #ver_code{width:114px;border:none;height: 27px;line-height: 28px;padding: 0;*float:left}
       #btn_get{width: 103px;height: 28px;line-height: 28px;display: inline-block;text-align: center;border-left: 1px solid #ccc;cursor:pointer;background-color:#f2f2f2;}
       #btn_file{display: inline-block;width: 68px;height: 28px;border-left:1px solid #ccc;background:#f2f2f2;text-align: center;line-height: 28px;cursor:pointer}
       #file{width: 218px;height: 28px;border:1px solid #ccc;display: inline-block;line-height: 30px;*float:left}
       #tel{width:74px;text-align: center}
       #des{width: 105px;height: 28px;display: none;background-color:#f2f2f2;line-height: 28px;text-align: center;border:1px solid #ccc;margin-left:10px;color:#838383;*float:left}
       #des b{font-weight:normal;}
       #code{float:left}
       #validate_sec{
         display: inline-block;
         #id_captcha_1,img,#js-captcha-refresh{float:left}
         #captcha_img{margin-left:2px;}
         #js-captcha-refresh{margin:3px 0 0 2px;cursor:pointer}
       }
       #email_border{
         display: inline-block;
         position: relative;
         *float:left;
         ul{position: absolute;top:30px;left:0;width:218px;border:1px solid #ccc;border-top: none;border-bottom: none;overflow: hidden;*display: none}
         ul li{
           list-style: none;
           background:#fff;
           margin:0;
           padding:0;
           height: 20px;
           line-height: 20px;
           text-indent: 3px;
           cursor:pointer;
         }
         li:hover{background:#c00;color:#fff}
         li:last-child{border-bottom: 1px solid #ccc}
       }
       #email_border+i,#email_border+i+p,#file+i,#file+i+p{*top:0;}
       .word{margin:0 4px;display: inline}
       .correct{display: inline-block;width: 12px;height: 12px;*position:relative;*top:-9px;background-image: url(../img/register_validate.jpg);background-position: 0px 12px;margin-left:10px;}
       .error{display: inline-block;*position:relative;*top:-9px;width: 12px;height: 12px;background-image: url(../img/register_validate.jpg);background-position: 0px 0px;margin-left:10px;}
     }
     #small_text input{width: 50px;text-align: center}
     input[type='button']{width: 120px;height: 40px;line-height: 40px;text-align: center;font-size: 14px;color:#fff;background-color: #c00;border:none;cursor:pointer;font-weight: bold;margin-left:120px}
     #contacts input{width: 75px;text-align: center}
     #contacts #name{width: 115px;margin-left:26px}
  }
}
/* 注册成功 */
.reg_success{
    width: 508px;
    height: 368px;
    border:1px solid #ccc;
    font-family: simsun;
    margin:auto;
    .title{
      height: 55px;
      line-height: 55px;
      text-align: center;
      font-size:14px;
      color:#c00;
      font-weight: bold;
      i{display: inline-block;width: 24px;height: 23px;background-image: url(../img/login_sign.png);margin-right: 5px;vertical-align: middle}
    }
    .des{
      width: 447px;
      height: 72px;
      line-height: 72px;
      text-align: center;
      border-top: 1px dotted #ccc;
      border-bottom: 1px dotted #ccc;
      font-size: 14px;
      margin:auto;
    }
    ul{width: 449px;margin:auto;font-size: 12px}
    ul li{
      list-style: none;
      text-indent: 50px;
      height: 34px;
      line-height: 34px;
      margin-top:10px;
      #alert{
        display: inline-block;
        width: 183px;
        height: 32px;
        border: 1px solid #ee9799;
        line-height: 32px;
        text-indent:0;
        text-align: center;
        background-color: #fff1f1;
        margin-left:15px;
        i{font-style: normal;display: inline-block;width: 18px;height: 18px;border-radius:9px;background:#c00;text-align: center;line-height: 18px;color:#fff;margin-right: 10px;font-family: arial}
      }
      input[type='radio']{vertical-align: -3px;}
      input[type='text']{width:102px;height: 28px;line-height: 28px;border:1px solid #ccc;margin-left:8px;font-size:12px;text-indent: 4px;font-family: simsun;}
      #confirm{display: inline-block;width: 120px;height: 40px;line-height: 40px;background-color: #c00;text-align: center;font-size:14px;color:#fff;font-weight:bold;text-indent:0;cursor:pointer;border: none}
    }
}
/* 修改登录密码样式 */
.login_left,
.address_left {
  width: 65%;
  float: left;
}
.login_left h6 {
  height: 35px;
  line-height: 35px;
  border-bottom: 1px solid #e3e3e3;
}
.login_left h6 span,
.address_left h6 span {
  font-size: 14px;
  font-family: "微软雅黑";
  font-weight: 700;
  color: #cc0000;
  border-bottom: 1px solid #cc0000;
  letter-spacing: 2px;
}
.login_left ul {
  padding: 20px 30px;
}
.login_left li {
  margin: 20px 0;
  text-align: left;
}
.login_left .error {
  background: #ffebea url(../img/vfc_error.gif) 10px center no-repeat;
  border: 1px solid #ebc1c2;
  height: 20px;
  line-height: 20px;
  margin-left: 70px;
  width: 150px;
  padding-left: 30px;
  color: #333;
  font-size: 12px;
  font-family: "微软雅黑";
  font-weight: normal;
}
.login_left span {
  color: #333;
  font-size: 14px;
  font-family: "微软雅黑";
  font-weight: normal;
  width: 90px;
  display: inline-block;
}
.login_left input {
  height: 30px;
  width: 280px;
  border: 1px solid #ccc;
  border-radius: 5px;
}
.login_left .sub_btn {
  height: 35px;
  width: 90px;
  margin-left: 115px;
  background: #cc0000;
  border: none;
  color: #fff;
  font-size: 14px;
  font-family: "微软雅黑";
  font-weight: 500;
}
.login_left a {
  font-size: 12px;
  font-family: "微软雅黑";
  font-weight: normal;
  color: #666;
  padding: 0 5px;
  text-decoration: underline;
}
#prompt{
  display: none;
  position: absolute;
  width: 250px;
  height: 74px;
  background-color:#fffde1;
  line-height: 74px;
  text-align: center;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.4);
  z-index: 998;
}
.login_left .re_span {
  width: 80px;
  padding-right: 5px;
  text-align: right;
}
.login_left .re_input {
  width: 180px;
}
.login_left .re_code {
  border: none;
  background: #fee4a7;
  width: 70px;
  font-size: 12px;
  font-family: "微软雅黑";
  font-weight: normal;
  color: #333;
}
.login_right,
.address_right {
  margin-top: 24px;
}
/* 收货地址管理的样式 */
.address_left h3 {
  margin-left: 15px;
  border-bottom: 1px solid #e3e3e3;
  height: 35px;
  line-height: 35px;
}
.address_left h3 span {
  display: inline-block;
}
.address_tit {
  margin-top: 15px;
  margin-left: 25px;
  font-size: 12px;
}
#add_newAddress {
  text-decoration: none;
  color: #cc0000;
}
#add_newAddress:hover {
  color: #00E8D7;
}
.address_left ul {
  padding: 20px 25px;
}
.address_left li {
  line-height: 30px;
  margin: 15px 0;
}
#address_pro {
  width: 95px;
}
#address_cit {
  width: 185px;
  margin-left: 10px;
}
.address_left li textarea {
  width: 300px;
  height: 70px;
  vertical-align: top;
}
.address_left li span {
  width: 90px;
  color: #333;
  font-size: 14px;
  font-family: "微软雅黑";
  font-weight: normal;
  display: inline-block;
}
.address_left input {
  height: 20px;
  width: 300px;
}
.default_set .set_radio {
  width: inherit;
  height: 12px;
  margin-left: 125px;
}
.address_left .sub_btn {
  width: 90px;
  height: 30px;
  background: #cc0000;
  border: none;
  color: #fff;
  font-size: 14px;
  font-family: "微软雅黑";
  margin-left: 125px;
  margin-top: 20px;
  cursor: pointer;
}
.address_count {
  margin: 45px 15px 150px 25px;
}
.address_list {
  margin-top: 20px;
}
.address_table {
  width: 100%;
  margin-bottom: 20px;
  background: #fff;
  font-size: 14px;
}
.address_table tr>th {
  background: #f5f5f5;
  padding: 2px;
  text-align: center;
  font-weight: 500 !important;
}
.address_table tr>td {
  padding: 30px 10px 30px;
  border-bottom: 1px solid #ddd;
  line-height: 1.42857143;
  text-align: center;
}
.address_table tr:hover {
  background: #f5f5f5;
}
.address_table tr>td a {
  font-size: 14px;
  text-decoration: none;
  color: inherit;
}
.address_table tr>td a:first-child:hover {
  color: lightskyblue;
}
.address_table tr>td a:last-child:hover {
  color: #ff0000;
}
.address_table tr .default_address {
  background: #cc0000;
}
.address_table tr .set_default {
  background: rgb(249,157,37);
}
.address_table tr .set_default:hover {
  background: #cc0000;
  cursor: pointer;
}
.address_table tr .default_address,
.address_table tr .set_default {
  width: 75px !important;
  height: 25px !important;
  color: #fff;
  border-radius: 5px;
  border: none;
}
