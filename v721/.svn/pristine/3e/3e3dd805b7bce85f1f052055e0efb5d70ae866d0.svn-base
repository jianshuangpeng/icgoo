.left{
  width: 65%;
  float: left;
  .title{
    color:#c00;
    font-size: 14px;
    text-indent: 1em;
    h6{
      margin-bottom:5px;
    }
    .line_gray{
      width: 100%;
      height:1px;
      background-color: #cbcbcb;
    }
    .line_red{
      width: 12.7%;
      height:1px;
      background-color: #c00;
    }
  }
  .main{
    margin-top:40px;
    div{
      height: 30px;
      margin-top:20px;
      line-height: 30px;
      text-indent: 1em;
      *line-height: 20px;
      label{
        font-size:14px;
        font-weight: bold;
        *vertical-align: super;
      }
      input{
        width: 353px;
        height: 28px;
        border:1px solid #ccc;
        border-radius: 3px;
        padding:0;
        text-indent: 1em;
        font-family: Microsoft YaHei;
        line-height: 28px;
      }
      input[type="button"]{
        width: 80px;
        height:30px;
        line-height: 30px;
        background-color:#c00;
        color:#fff;
        border:none;
        border-radius:5px;
        text-align: center;
        text-indent: 0;
        cursor:pointer;
        margin-top:30px;
      }
    }
    #owner_name{
      height: 62px;
      label{
        vertical-align: top;
      }
      textarea {
        width: 339px;
        height: 60px;
        border:1px solid #ccc;
        font-family: Microsoft YaHei;
        resize:none;
        line-height: 30px;
        border-radius:3px;
        padding-left: 1em;
        overflow: hidden;
      }
    }
  }
}
.right{
    float: right;
    width: 23%;
    .banner{
      width: 100%;
      height: 166px;
      position: relative;
      overflow: hidden;
      margin-top:24px;
      ul{
        float: left;
      }
      .big{
        li{
          width: 250px;
          height:166px;
          overflow: hidden;
        }
        img{
          width: 250px;
          height: 166px;
        }
      }
      .icon{
        position: absolute;
        bottom: 10px;
        left: 188px;
        a{
          width: 9px;
          height: 9px;
          display: inline;
          float: left;
          margin: 0 3px;
          overflow: hidden;
          background: url("../img/icon_1.png") no-repeat;
          background-position: top right;
        }
        a.active{
          background:  url("../img/icon_1.png") no-repeat top left;
        }
      }
    }
  }
