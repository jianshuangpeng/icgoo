.main{
		width:1045px;
		float: left;
		margin-left:12px;
		font-size: 14px;
		font-weight: normal;
		#left{
			float:left;
			width: 750px;
			#already_reg{
					margin-top:56px;
					padding-left:140px;
					i{
							display: inline-block;
							width: 49px;
							height: 50px;
							background: url('../img/smile.png');
							float:left;
					}
					span{
							display: inline-block;
							height: 50px;
							line-height: 50px;
							margin-left:20px;
							float:left;
							font-size: 12px;
							font-family: simsun;
					}
			}
		}
		.left:first-child{margin-top:8px}
		.left:last-child{margin-bottom:20px}
		.left{
				float: left;
				width: 100%;
				margin-top:10px;
				font-family: simsun;
				margin:28px 0 0 8px;
				h6{margin-bottom: 5px;font-family: simsun;display: inline-block;margin-right:20px;cursor:pointer;width: 60px;padding: 0;*float: left;}
				.active{color:#c00}
				.line_gray{width: 100%;height: 1px;background: #CBCBCB}
				.line_red{width: 60px;height: 1px;background: #cc0000;position: relative;}
				span{font-size: 12px;}
				a{color:#0095f3;margin-left:20px}
				ul{
					margin-top:18px;
					position: relative;
					height: 204px;
					input[type='text'],input[type='password']{width: 198px;height: 28px;border:1px solid #ccc;text-indent:4px;padding: 0;line-height: 28px;font-size: 12px}
					input[type='text']{font-family: simsun}
					.border,input[name='address']{width: 338px;height: 28px;border:1px solid #ccc}
					input[name='file']{width: 270px;height: 28px;border:none;float:left}
					input[type='submit']{width: 105px;height: 32px;line-height: 32px;background: #c00;text-align: center;border:none;color:#fff;margin:20px 50px 0 150px;cursor: pointer;font-size: 12px;font-weight: bold}
					#btn_file{width: 67px;border-left: 1px solid #ccc;background-color: #f2f2f2;height: 28px;text-align: center;line-height: 28px;display: inline-block;font-size: 12px;cursor:pointer}
					#validate_border{
						width: 198px;
						height: 28px;
						border:1px solid #ccc;
						display: inline-block;
						input{
							width: 95px;
							height: 28px;
							border:none;
						}
						span{
							display: inline-block;
							background-color: #f2f2f2;
							width: 102px;
							height: 28px;
							border-left:1px solid #ccc;
							text-align: center;
							line-height: 28px;
							font-size: 12px;
							cursor:pointer;
						}
					}
					#des{width: 105px;height: 28px;display: none;background-color:#f2f2f2;line-height: 28px;text-align: center;border:1px solid #ccc;margin-left:10px;color:#838383;font-size:12px;font-family: simsun}
					#des b{font-weight:normal;}
					.correct{display: inline-block;width: 12px;height: 12px;background-image: url(../img/register_validate.jpg);background-position: 0px 12px;margin-left:10px;margin-top:7px;}
			    .error{display: inline-block;width: 12px;height: 12px;background-image: url(../img/register_validate.jpg);background-position: 0px 0px;margin-left:10px;}
					p{display: inline;color:#c00;margin-left:5px;position: relative;top:-2px;font-size: 12px;font-family: simsun;*top:0}
				}
				.hide{display: none}
				ul li{height: 30px;line-height: 30px;margin-bottom: 5px;}
				#indicatorContainer{
					width:130px;
					position: absolute;
					top:22px;
					right:50px;
					#center{
						position: absolute;
						width: 130px;
						text-align: center;
						top:35px;
						font-size:12px;
						font-family:simsun;
						p{font-size: 16px;color:#c00;font-weight: bold;line-height: 20px;display: none;}
					}
				}
				.clear{clear:both}
				#headImage{height:50px;line-height: 50px}
				.item{width: 130px;float: left;font-weight: bold;font-size:12px;text-align: right}
				.content{
					width: 450px;
					float: left;
					padding-left: 20px;
					font-size: 12px;
					.small_text{width:50px;}
					.word{float:left}
					#tel{width: 70px;}
				}
				.content .link{display: inline-block;color:#39f;margin-left:50px}
				.content .link:hover{text-decoration: underline;color:#c00}
				.content input{float:left}
				.content .edit{width: 84px;height:25px;border:1px solid #cbcbcb;border-radius:5px;display: inline-block;line-height: 25px;text-align: center;float: left;margin-top:11px}
				/*以下是修改头像页面的样式*/
				.left_change{
						width: 350px;
						float: left;
						.btn{width: 127px;height: 31px;border:1px solid #CBCBCB;border-radius:4px;background: url(../img/u214.png) no-repeat 12px center;line-height: 31px;cursor: pointer;margin-top:18px;}
						.btn span{margin-left:35px;color:#333}
						p{font-size:13px;color:#666;height: 40px;line-height: 40px}
						#preview{border: 1px solid #cbcbcb}
						#preview,.img,img{width: 350px;height: 180px}
				}
				.right_change{
						width: 300px;
						float: left;
						margin:18px 0 0 50px;
						span{font-weight: bold}
						p{font-size:13px;color:#666;margin:5px 0 20px 0}
						.content{
								.small_pic{width: 60px;float: left;margin-right:30px}
								#headImage1{border:1px solid #cbcbcb;}
								#headImage2{border:1px solid #cbcbcb;background:#fff}
								#headImage1,#headImage1 img,.img1{width: 60px;height: 60px;}
								#headImage2,#headImage2 img,.img2{width: 60px;height: 60px;border-radius:30px;behavior:url(http://demo.dzji.com/statics/js/ie-css3.htc);}
								span{color:#666;font-weight:normal;font-size:13px;margin-top:10px;display: block}
						}
				}
		}
		.right{
				float: right;
				width: 23%;
				.service_info{
						width:100%;
						font-family: simsun;
						padding-top:5px;
						p{height: 30px;line-height: 30px;font-size:12px}
						p:first-child{margin-bottom:10px}
						p:first-child span{font-weight: bold;font-size:14px}
						p i{display: inline-block;width: 19px;height: 19px;background-image:url(../img/earphone.png)}
						p label{display: inline-block;width:60px;text-align:right}
						p span{margin-left:10px}
						p a{display: inline-block;width:14px;height: 15px;background:url('../img/qq.gif');margin-left:20px}
				}
				.banner{
					width: 100%;
					height: 166px;
					position: relative;
					overflow: hidden;
					margin-top:24px;
					ul{
						float: left;
					}
					.big{
						li{
							width: 250px;
							height:166px;
							overflow: hidden;
						}
						img{
							width: 250px;
							height: 166px;
						}
					}
					.icon{
						position: absolute;
						bottom: 10px;
						left: 188px;
						a{
							width: 9px;
							height: 9px;
							display: inline;
							float: left;
							margin: 0 3px;
							overflow: hidden;
							background: url("../img/icon_1.png") no-repeat;
							background-position: top right;
						}
						a.active{
							background:  url("../img/icon_1.png") no-repeat top left;
						}
					}
				}
			}
}
#prompt_change{
		display: none;
		width: 410px;
		height: 154px;
		background-color: rgba(0,0,0,.5);
		position: fixed;
		left:50%;
		top:50%;
		margin-left:-305px;
		margin-top:-107px;
		.content{
			width: 390px;
			height: 114px;
			margin: 10px auto;
			background-color: #fff;
			padding-top: 20px;
			position:relative;
			h3{
				text-align: center;
				margin-top:25px;
				*margin-top:40px;
				font-weight: normal;
				.success{color:#3399FF;}
				#second{color:#c00}
			}
			p{text-align: center;font-size: 12px;font-family: simsun;height: 30px;line-height: 30px;}
			i{width:16px;height: 15px;display: inline-block;background: url('../img/close_x.png');position: absolute;top:10px;right:10px;cursor:pointer}
		}
}
@media screen and (max-width:1190px) {
	.container{
		.main{
				margin-left: 4px;
				.left{
						width:622px;
						#indicatorContainer{right:20px;}
						.content{
								width:350px;
						}
				}
				#left{
						width: 630px;
				}
		}
	}
}
